    <None Remove="FinancialData\Years\MonthlyStockQuotes2016.json" />
    <None Remove="FinancialData\Years\MonthlyStockQuotes2017.json" />
    <None Remove="FinancialData\Years\MonthlyStockQuotes2018.json" />
    <None Remove="FinancialData\Years\MonthlyStockQuotes2018cs" />
    <None Remove="FinancialData\Years\MonthlyStockQuotes2019.json" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="FinancialData\HistoricDividend.json">
