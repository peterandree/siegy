    <Content Include="FinancialData\Years\MonthlyStockQuotes2015.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="FinancialData\Years\MonthlyStockQuotes2016.json">
