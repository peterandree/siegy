<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <EnforceCodeStyleInBuild>false</EnforceCodeStyleInBuild>
    <EnableNETAnalyzers>true</EnableNETAnalyzers>
    <OutputType>Library</OutputType>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="FinancialData\Years\MonthlyStockQuotesFuture.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="FinancialData\HistoricDividend.json" />
    <None Remove="FinancialData\HistoricMonthlyRate.json" />
    <None Remove="FinancialData\HistoricOneTimeRate.json" />
    <None Remove="FinancialData\HistoricProfitSharingStocks.json" />
